#Database set up
dbi.default.driver = "dbmsqt"
dbi.database.local_db.source = "local_db.db"

#Application localisation
fglrun.localization.file.count = 2
fglrun.localization.file.1.name = "common_per.42s"
fglrun.localization.file.2.name = "common_string.42s"

#Only enable warnKeyNotFound for debugging.
fglrun.localization.warnKeyNotFound = TRUE

#Mobile specific ENV set up (This only effects when deployed from API or APK files)
mobile.environment.FGLIMAGEPATH = "$FGLAPPDIR:$FGLAPPDIR/my_image2font.txt"